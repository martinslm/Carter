<Page x:Class="Carter.Telas.HistoricoDeSalarios"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Carter.Telas"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HistoricoDeSalarios">

    <Grid>
        <Grid Background="#FFFAFA">
            <Grid.RowDefinitions>
                <RowDefinition Height="30"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
        <StackPanel Margin="4,0,0,5" x:Name="stackPanel4" Grid.Column="0" Grid.ColumnSpan="2">
            <Border BorderBrush="#90EE90" BorderThickness="1" CornerRadius="1">
                <WrapPanel x:Name="ListagemHistoricoSalarios">
                    <Label Width="60" Content="Salário"/>
                    <Label Width="90" Content="Data Cadastro"/>
                </WrapPanel>
            </Border>
        </StackPanel>
    </Grid>
</Page>
