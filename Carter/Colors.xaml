<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Carter">

    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#3CB371"/>
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#3CB371"/>
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#3CB371"/>
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#3CB371"/>
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFF"/>
    <!-- accent -->
    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="#3CB371"/>
</ResourceDictionary>